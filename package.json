{
  "name": "starfyre",
  "displayName": "starfyre",
  "description": "A VSCode extension for syntax highlighting Starfyre files.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sparckles/vscode-starfyre"
  },
  "publisher": "Sanskar Jethi <sansyrox@gmail.com>",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "starfyre",
        "aliases": [
          "Starfyre",
          "starfyre"
        ],
        "extensions": [
          ".fyre"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "starfyre",
        "scopeName": "source.starfyre",
        "path": "./syntaxes/starfyre.tmLanguage.json"
      }
    ]
  }
}
